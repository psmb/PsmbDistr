{
    "name": "psmb/psmbdistr",
    "description": "Psmb.ru Distribution",
    "config": {
        "vendor-dir": "Packages/Libraries",
        "bin-dir": "bin"
    },
    "repositories": [
        {
            "url": "https://github.com/dimaip/neos-seo",
            "type": "git"
        }
    ],
    "require": {
        "neos/neos-development-collection": "~3.2@dev",
        "neos/flow-development-collection": "@dev",

        "flowpack/elasticsearch-contentrepositoryadaptor": "@dev",
        "neos/googleanalytics": "^2.0.0",
        "neos/redirecthandler": "@dev",
        "neos/redirecthandler-databasestorage": "@dev",
        "neos/redirecthandler-neosadapter": "@dev",
        "neos/neos-ui": "dev-master",
        "neos/neos-ui-compiled": "dev-master",

        "flowpack/nodetemplates": "dev-master@dev",
        "sfi/youtube": "@dev",
        "sfi/grid": "@dev",
        "psmb/term": "^3.0",
        "psmb/nodetypes": "^3.1",
        "psmb/createnodebutton": "@dev",
        "flowpack/jobqueue-common": "@dev",
        "psmb/newsletter": "^2.2",
        "psmb/mediaquery": "^2.0",
        "psmb/mapreduce": "^2.0",
        "psmb/carousel": "@dev",
        "dimaip/groupby": "^2.0",
        "opsdev/cache-breaker": "^3.0",
        "neos/seo": "dev-master@dev",
        "yeebase/graylog": "@dev"
    },
    "extra": {
        "neos": {
            "default-disabled-packages": [
                "flowpack/neos-frontendlogin",
                "neos/nodetypes"
            ]
        }
    },
    "scripts": {
        "post-update-cmd": "Neos\\Flow\\Composer\\InstallerScripts::postUpdateAndInstall",
        "post-install-cmd": "Neos\\Flow\\Composer\\InstallerScripts::postUpdateAndInstall",
        "post-package-update": "Neos\\Flow\\Composer\\InstallerScripts::postPackageUpdateAndInstall",
        "post-package-install": "Neos\\Flow\\Composer\\InstallerScripts::postPackageUpdateAndInstall"
    }
}
