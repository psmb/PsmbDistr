prototype(Sfi.Site:MailLayout) < prototype(Neos.Fusion:Template) {
    templatePath = 'resource://Sfi.Site/Private/TypoScript/Newsletter/Objects/MailLayout.html'
    value = ${value}

    logoUri = Neos.Fusion:ResourceUri {
        path = 'resource://Sfi.Site/Public/Images/psmb-newsletter.png'
    }

    unsubscribeLink = Psmb.Newsletter:UnsubscribeLink
    editSubscriptionLink = Psmb.Newsletter:EditSubscriptionLink
}
